{
  "name": "get-port",
  "version": "3.1.1",
  "description": "Get an available port",
  "license": "MIT",
  "repository": "sindresorhus/get-port",
  "typings": "dist/index.d.ts",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --ci --coverage --no-cache",
    "compile": "rm -rf dist && tsc -d",
    "prepublish": "npm run compile"
  },
  "main": "dist/index.js",
  "keywords": [
    "port",
    "find",
    "finder",
    "portfinder",
    "free",
    "available",
    "connection",
    "connect",
    "open",
    "net",
    "tcp",
    "scan",
    "rand",
    "random",
    "preferred",
    "chosen"
  ],
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/__test__/.*.test.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
